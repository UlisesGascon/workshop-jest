    "url": "https://github.com/UlisesGascon/workshop-jest/issues"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "/docs/**/*.js"
    ]
  },
  "homepage": "https://github.com/UlisesGascon/workshop-jest#readme",
  "dependencies": {
    "got": "^11.5.2",
    "jest": "^26.4.2",
    "jest-puppeteer": "^4.4.0",
